                {{#each products.payload}}
                <div class="col">
                    <div class="contexto__producto">
                        <img class="imagen__producto" src="img/"+{{this.thumbnail}} alt={{this.thumbnail.imageDescription}}>
                        <h2 class="product__title">{{this.title}}</h2>
                        <p class="product__price">
                            Precio: {{this.price}}
                        </p>
                        <p class="product__description">
                            Descripción: {{this.description}}
                        </p>
                        {{#if this.status}}
                        <p>Disponible</p>
                        <form class="addToCartForm" id="{{this._id}}" data-owner="{{this.owner}}">
                            <label>Cantidad:
                                <input type="text" name="quantity" class="quantity" />
                            </label>
                            <input type="submit" value="Agregar al carrito" />
                        </form>
                        {{else}}
                        <p>No disponible</p>
                        {{/if}}
                        <button class="btn btn-warning">
                            Mas información
                        </button>
                    </div>
                </div>
                {{/each}}
                {{!-- {{#each productos}}
